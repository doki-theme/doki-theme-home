{
  "name": "svelte-app",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "buildThemes": "ts-node --project buildSrc/tsconfig.json buildSrc/src/BuildThemes.ts",
    "sync": "ts-node --project buildSrc/tsconfig.json buildSrc/src/AssetUpload.ts",
    "build": "rollup -c",
    "release": "npm run buildThemes && npm run build && npm run sync",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear -s",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/marked": "^2.0.3",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "marked": "^2.0.5",
    "sirv-cli": "^1.0.0",
    "svelte-navigator": "^3.1.5"
  }
}

